{
  "name": "react-table",
  "version": "0.1.0",
  "description": "Hooks for building lightweight, fast and extendable datagrids for React",
  "license": "MIT",
  "homepage": "https://github.com/tannerlinsley/react-table#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tannerlinsley/react-table.git"
  },
  "keywords": [
    "react",
    "table",
    "react-table",
    "datagrid"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "Tanner Linsley",
  "module": "dist/react-table.esm.js",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "postinstall": "node ./scripts/postinstall.js || exit 0"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@scarf/scarf": "^0.1.5"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "husky": "^4.2.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "exit 0"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
